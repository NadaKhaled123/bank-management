div.clc-cp-multi .footer { font-size: 13px; border-bottom-left-radius: 2px; border-bottom-right-radius: 2px; color: var(--black-500)  !important; }

div.clc-cp-multi .footer .arrow-ico { color: var(--black-200); }

div.clc-cp-multi.check-out-purple { background-image: linear-gradient(89.81deg, rgb(75, 53, 136) 30%, rgb(102, 73, 184) 90%); background-position: center 0px; }

div.clc-cp-multi.check-out-red { background-image: linear-gradient(89.81deg, rgb(197, 38, 38) 30%, rgb(219, 67, 67) 90%); background-position: center 0px; }
