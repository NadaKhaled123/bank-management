  #OldHeader ul.headerMenu li { padding-right: 0.02%; }
  ul.headerMenu li > a { font-size: calc(13px - 0.15vw) !important; }
}

@media (max-width: 1023px) {
  .home-page { padding-top: 0px !important; }
  #topAnnouncement a { white-space: normal !important; font-size: 14px !important; }
  #topAnnouncement > div:first-child { line-height: initial !important; font-size: small !important; }
  #topAnnouncement a#greenHeaderClose { line-height: 20px !important; font-size: 18px !important; transform: scale(0.7) !important; }
}

@media (max-width: 767px) {
  #topAnnouncement > div > div { padding: 8px 40px 0px 10px !important; }
}

@media only screen and (min-width: 768px) {
  .content-head-link .ad-items { width: 50% !important; }
}

@media screen and (max-width: 1023px) {
  .fixed { position: fixed !important; }
  body.home-page.header-fixed { padding-top: 32px !important; }
  .norml { position: inherit !important; }
}

@media (max-width: 1023px) {
  #OldHeader .responsiveHeader { left: initial !important; }
}

.have-company { display: flex; justify-content: space-between; }

.have-company #AboutAuthorBox { width: 49%; margin-right: 10px; }

.company-section { width: calc(50% - 10px); padding: 15px; display: flex; justify-content: space-between; background: rgb(245, 245, 245); height: max-content; }

.company-details { width: calc(100% - 90px); }

.company-details a { color: rgb(33, 33, 33); }

.company-photo, .company-photo > a img { width: 80px; height: 80px; overflow: hidden; border: 1px solid rgb(221, 221, 221); border-radius: 100px; }

.company-name { font-size: 18px; font-weight: 700; border-bottom: 1px solid rgb(236, 236, 236); padding-bottom: 10px; margin-bottom: 10px; }

.company-description { font-size: 14px; line-height: 1.5; }

@media only screen and (min-width: 768px) {
  .have-company .author-bio-content .author-bio-icons { position: relative; top: 5px; left: 100px; }
}

@media only screen and (max-width: 768px) {
  .have-company { display: block; }
  .have-company #AboutAuthorBox, .company-section { width: 100%; margin-right: 0px; margin-bottom: 10px; }
  .company-section { margin-top: 50px; }
  .have-company .author-bio-content .author-bio-icons { position: absolute; left: inherit; right: 0px; }
}
